public class M10 {
    public static void main(String[] args) {
        Double money = 100000d;
        int time1 =0;
        int time2 = 0;
        while (money > 50000){
            money = money * 0.95;
            time1++;
        }
        while (money <= 50000){
            money = money - 1000;
            if (money < 1000){
                break;
            }
            time2++;
        }
        int num = time1 + time2;
        System.out.println("总次数为：" + num);
    }
}
