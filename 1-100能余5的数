public class M11 {
    public static void main(String[] args) {
        int num = 0;
        for (int i = 1;i <= 100;i++){
            if (i % 5 ==0){
                System.out.print(i + "\t");
                num++;
                if (num % 5 == 0){
                    System.out.println();
                }
            }

        }
    }

}
