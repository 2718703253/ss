public class M6 {
    public static void main(String[] args) {
        int i , j=1;
        int sum = 0;
        for (i = 1;i <= 9 ;i++){
            for (j = 1;j <= i;j++){
                sum = i * j;
                System.out.print(j+" * "+i+" = "+sum+"\t");
            }
            System.out.println();
        }
    }
}
